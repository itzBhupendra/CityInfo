using System;
using System.Collections.Generic;

public static class ApiService
{
    public static List<LogEntry> FetchLogs(string correlationId)
    {
        // Simulate fetching logs
        Task.Delay(2000).Wait();

        return new List<LogEntry>
        {
            new LogEntry { DateTime = DateTime.Now, LogLevel = "Info", CorrelationId = correlationId, Message = "Sample log message 1", LogFolderName = "Server1" },
            new LogEntry { DateTime = DateTime.Now, LogLevel = "Error", CorrelationId = correlationId, Message = "Sample log message 2", LogFolderName = "Server2" },
        };
    }
}
